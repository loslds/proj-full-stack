{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["es2020"],
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["vite.config.ts", "src/**/*"]
}




